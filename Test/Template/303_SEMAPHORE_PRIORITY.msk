SNIPPET_START PARTITION1_PARTITION_IDLEPROCESS_CODE
		// Sets LEDs pattern
		TEST_LED1_OFF();
		TEST_LED2_OFF();

		// Gets semaphore identifier
		GET_SEMAPHORE_ID((SEMAPHORE_NAME_TYPE) "SEMAPHORE1", &SEMAPHORE_ID, &RETURN_CODE);
		if (RETURN_CODE != NO_ERROR) {
			while (true) {
			}
		}

		// Counts
		UI32_COUNTER1++;

		// Verifies counter
		if (UI32_COUNTER1 == TEST_COUNTER_SMALLMEDIUM) {

			// Signals semaphore
			SIGNAL_SEMAPHORE(SEMAPHORE_ID, &RETURN_CODE);
			if (RETURN_CODE != NO_ERROR) {
				while (true) {
				}
			}

			// Gets semaphore status
			GET_SEMAPHORE_STATUS(SEMAPHORE_ID, &SEMAPHORE_STATUS, &RETURN_CODE);
			if (RETURN_CODE != NO_ERROR) {
				while (true) {
				}
			}
			if (SEMAPHORE_STATUS.CURRENT_VALUE > 1) {
				while (true) {
				}
			}

			// Resets counter
			UI32_COUNTER1 = 0;
		}
SNIPPET_END
